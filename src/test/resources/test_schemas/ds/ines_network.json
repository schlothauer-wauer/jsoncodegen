        },
        "tenantId": {
          "description": "what is the related tenant to that entry",
          "type": "string",
          "format": "uuid",
          "__ref": "./shared/tenant.json",
          "__tags": [
            "notDisplayed"
          ]
        }
      },
      "__tags": [
        "rest",
        "mongodb"
      ],
      "__version": 4
