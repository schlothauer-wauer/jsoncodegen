          "items": {
            "$ref": "#/definitions/InesSubIntersection"
          }
        },
        "signalPrograms": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/InesSignalProgram"
          }
        },
        "stages": {
          "description": "???",
