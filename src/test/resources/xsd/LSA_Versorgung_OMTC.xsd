					<xs:documentation>Rückrechenverfahren für Koordinierung</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:minInclusive value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OeVType">
		<xs:sequence>
			<xs:element name="OeVMeldepunktListe" type="OeVMeldepunktListeType">
				<xs:annotation>
					<xs:documentation>
						Meldepunkte zur ÖV-Erfassung
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OeVStreckenListe" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						Erfassungsstrecken von der Voranmeldung bis zur
						Abmeldung
					</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="OeVStrecke" type="OeVStreckeType" maxOccurs="unbounded"/>
						<xs:element name="MaxFahrzeuge" default="4" minOccurs="0">
							<xs:annotation>
								<xs:documentation>
									Max. Anzahl von verwalteten
									Fahrzeugen Defaultwert ist 4
								</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:positiveInteger">
									<xs:minInclusive value="1"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="OeVParaSatzListe" minOccurs="0">
